name: yolov5

entry_points:
  main:
    parameters:
      model-cfg: { type: string, default: "yolov5n.yaml"}
      epochs: { type: int, default: 64}
      batch-size: { type: int, default: 16}
      data: { type: string, default: 'custom.yaml'}
      weights: {type: string, default: ''}

    command: "python3 train.py --data {data} --epochs {epochs} --weights {weights} --cfg {model-cfg} --batch-size {batch-size} --cache --save-period=32"
